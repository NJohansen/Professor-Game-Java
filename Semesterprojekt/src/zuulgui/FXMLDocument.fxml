<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.canvas.Canvas?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane fx:id="gp" gridLinesVisible="true" style="-fx-background-position: center; -fx-background-image: url('wow2.jpg'); -fx-background-size: cover;" stylesheets="@style.css" xmlns="http://javafx.com/javafx/9" xmlns:fx="http://javafx.com/fxml/1" fx:controller="zuulgui.FXMLDocumentController">
   <columnConstraints>
      <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
   </rowConstraints>
   <children>
      <GridPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="800.0" prefWidth="1140.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="672.0" minWidth="10.0" prefWidth="637.0" />
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="327.0" minWidth="0.0" prefWidth="266.0" />
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="266.0" minWidth="10.0" prefWidth="218.0" />
        </columnConstraints>
        <rowConstraints>
            <RowConstraints maxHeight="348.0" minHeight="10.0" prefHeight="91.0" vgrow="SOMETIMES" />
          <RowConstraints maxHeight="348.0" minHeight="10.0" prefHeight="78.0" vgrow="SOMETIMES" />
          <RowConstraints maxHeight="266.0" minHeight="0.0" prefHeight="206.0" vgrow="SOMETIMES" />
          <RowConstraints maxHeight="153.0" minHeight="0.0" prefHeight="153.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Canvas fx:id="c" height="640.0" width="640.0" GridPane.rowSpan="3" />
            <AnchorPane prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="2" />
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="15.0" GridPane.rowIndex="3">
               <children>
                  <Button fx:id="buttonSavePressed" mnemonicParsing="false" onAction="#handleButtonAction" text="SAVE" HBox.hgrow="ALWAYS">
                     <padding>
                        <Insets bottom="5.0" left="30.0" right="30.0" top="5.0" />
                     </padding>
                     <HBox.margin>
                        <Insets />
                     </HBox.margin>
                  </Button>
                  <Button fx:id="highscoreButton" mnemonicParsing="false" onAction="#highscoreButtonPressed" text="HIGHSCORE" HBox.hgrow="ALWAYS">
                     <padding>
                        <Insets bottom="5.0" left="30.0" right="30.0" top="5.0" />
                     </padding>
                     <HBox.margin>
                        <Insets />
                     </HBox.margin>
                  </Button>
                  <Button mnemonicParsing="false" text="YES" HBox.hgrow="ALWAYS">
                     <padding>
                        <Insets bottom="5.0" left="30.0" right="30.0" top="5.0" />
                     </padding>
                     <HBox.margin>
                        <Insets />
                     </HBox.margin>
                  </Button>
                  <Button fx:id="quitGameButton" mnemonicParsing="false" onAction="#quitGameButton" text="QUIT" HBox.hgrow="ALWAYS">
                     <padding>
                        <Insets bottom="5.0" left="30.0" right="30.0" top="5.0" />
                     </padding>
                     <HBox.margin>
                        <Insets />
                     </HBox.margin>
                  </Button>
               </children>
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
            </HBox>
            <TextArea fx:id="textArea" disable="true" opacity="1" prefHeight="200.0" prefWidth="200.0" styleClass="text-area" GridPane.columnIndex="1" GridPane.rowIndex="1" />
         </children>
      </GridPane>
   </children>
</GridPane>
